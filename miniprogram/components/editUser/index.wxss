.form-page{
  position: fixed;
  left: 0;
  top: 0;
  width: 100vw;
  height: 100vh;
  background: #fff;
}
.form-content{
  display: flex;
  flex-flow: column;
  gap: 20rpx;
}
.form-item{
  display: flex;
  gap: 20rpx;
  border-bottom: 2rpx solid #ccc;
  padding: 20rpx;
}
.form-item radio-group {
  display: flex;
  gap: 20rpx;
}
.btn-group{
  margin-top: 30rpx;
}
.btn-group button{
  width: 90%;
  margin-bottom: 30rpx;
}
